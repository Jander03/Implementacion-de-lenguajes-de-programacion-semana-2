class Conjunto:
    def __init__(self, elementos=None):
        if elementos is None:
            self.elementos = []
        else:
          self.elementos = elementos

    def __str__(self):
        return "{" + ", ".join(str(e) for e in self.elementos) + "}"

    def union(self, otro):
        return Conjunto(self.elementos + otro.elementos)

    def interseccion(self, otro):
        inter = [e for e in self.elementos if e in otro.elementos]
        return Conjunto(inter)

    def diferencia(self, otro):
        diff = [e for e in self.elementos if e not in otro.elementos]
        return Conjunto(diff)

    def producto_cartesiano(self, otro):
        prod = [(e1, e2) for e1 in self.elementos for e2 in otro.elementos]
        return Conjunto(prod)

A = Conjunto([1, 2, 3, 4])
B = Conjunto([3, 4, 5])

print("A =", A)
print("B =", B)
print("A ∪ B =", A.union(B))
print("A ∩ B =", A.interseccion(B))
print("A - B =", A.diferencia(B))
print("B - A =", B.diferencia(A))
print("A × B =", A.producto_cartesiano(B))
